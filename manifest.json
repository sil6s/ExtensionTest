{
  "name": "ITSC Project Management",
  "description": "Base Level Extension",
  "version": "1.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "service_worker.js"
  },
  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "css": ["styles.css"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "scripting"
  ]
  
  
}